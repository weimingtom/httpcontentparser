<?xml version="1.0" encoding="UTF-8" ?>
<config installed="true" first="true">
    <rules>
        <rule name="onlinetime" enable="true" />
        <rule name="blackurl" enable="true" />
        <rule name="whiteurl" enable="true" justpass="false">
            <url>hello</url>
            <url>www.sina.com</url>
            <url>www.sohu.com</url>
        </rule>
        <rule name="search" enable="true">
