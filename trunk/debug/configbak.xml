                <searchengine name="yahoo" enable="true" />
            </searchengines>
        </rule>
        <rule name="imagecheck">
            <check imagetype="jpg" enable="true" />
            <check imagetype="bmp" enable="true" />
            <check imagetype="gif" enable="true" />
            <check imagetype="png" enable="true" />
            <scope enable="true" min="0" max="0" />
